#include<iostream>
using namespace std;

struct rectangle
{
    int l;
    int b;
};

int main()
{
    struct rectangle *p;          // dynamic allocation of pointers    
    p=(struct rectangle*)malloc(sizeof(struct rectangle));
    p->l=10;
    p->b=5;
    cout<<p->l<<" "<<p->b;
    
    return 0;
    
}
