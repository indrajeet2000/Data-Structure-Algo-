//unsorted search

#include <iostream>

using namespace std;

int search(int arr[],int s)
{
  cout<<"enter no. to be searched in the list\n";
  int n;
  cin>>n;
  int i;
    for(i=0;i<s;i++)
    {
        if(n==arr[i])
        {
         cout<<"found at -> "<<i+1;
         return 0;
        }
    }
    
    {
     cout<<"not found\n";
     return 0;
    }
    
}
    

int main()
 {
    int s;
    cout<<"enter size of the array\n";
    cin>>s;
    int arr[s];
    cout<<"enter all the elements one by one\n";
    for(int i=0;i<s;i++)
    {
        cin>>arr[i];
    }
    search(arr,s);
    return 0;
}
