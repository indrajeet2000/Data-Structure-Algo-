void merge(struct node *p,struct node *q)
{
 struct node *last;
 if(p->data < q->data)
 {
 third=last=p;
 p=p->next;
 third->next=NULL;
 }
 else
 {
 third=last=q;
 q=q->next;
 third->next=NULL;
 }
 while(p && q)
 {
 if(p->data < q->data)
 {
 last->next=p;
 last=p;
 p=p->next;
 last->next=NULL;

 }
 else
 {
 last->next=q;
 last=q;
 q=q->next;
 last->next=NULL;

 }
 }
 if(p)last->next=p;
 if(q)last->next=q;

}

void display(struct node *p)
{
    while(p)
    {
        cout<<p->data<<" ";
        p=p->next;
    }
}

int main()
{
    int n;
    cout<<"enter size\n";
    cin>>n;
    int a[n];
    cout<<"enter elements\n";
    for(int i=0;i<n;i++)
    {
        cin>>a[n];
    }
    
    int m;
    cout<<"enter the size\n";
    cin>>m;
    int b[m];
    cout<<"enter the elements\n";
    for(int i=0;i<m;i++)
    {
        cin>>b[i];
    }
    
    sort(a,a+n);
    sort(b,b+m);
    
    create1(a,n);
    create2(b,m);
    
    merge(first,second);
    display(third);
    return 0;
}
