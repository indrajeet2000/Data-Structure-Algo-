// in this we will merge 2 sorted linked list in a sorted order
#include<iostream>
#include<algorithm>
using namespace std;

struct node
{
    int data;
    struct node *next;
}*first=NULL,*second=NULL,*third=NULL;

void create1(int a[],int n)
{
    struct node *t,*last;
    first=new node;
    first->data=a[0];
    first->next=NULL;
    last=first;
    
    int i;
    for(i=1;i<n;i++)
    {
        t=new node;
        t->data=a[i];
        t->next=NULL;
        last->next=t;
        last=t;
    }
}

void create2(int b[],int m)
{
    struct node *t,*last;
    second=new node;
    second->data=b[0];
    second->next=NULL;
    last=second;
    
    int i;
    for(i=1;i<m;i++)
    {
        t=new node;
        t->data=b[i];
        t->next=NULL;
        last->next=t;
        last=t;
    }
}

