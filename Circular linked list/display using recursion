#include<iostream>
using namespace std;

struct node
{
    int data;
    struct node *next;
}*head;

void create(int a[], int n)
{
    struct node*t,*last;
    head=new node;
    head->data=a[0];
    head->next=head;
    last=head;
    
    int i;
    for(i=1;i<n;i++)
    {
        t=new node;
        t->data=a[i];
        t->next=last->next;
        last->next=t;
        last=t;
    }
}


void rdisplay(struct node *p)
{
    static int flag=0;
    if(p!=head || flag==0)
    {
        flag=1;
        cout<<p->data<<" ";
        rdisplay(p->next);
    }
    flag=0;
}

int main()
{
   int a[]={2,3,4,5,6};
   create(a,5);
   rdisplay(head);
   return 0;
}
